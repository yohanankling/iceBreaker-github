<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".contactUs.Contact">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/userslist"
        android:layout_width="0dp"
        android:layout_height="100dp"
        android:layout_weight="1"
        android:background="@color/bar"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:id="@+id/back"
            android:layout_width="53dp"
            android:layout_height="52dp"
            android:adjustViewBounds="true"
            android:background="#00FFFFFF"
            android:scaleType="centerInside"
            android:src="@drawable/back_btn"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/Contact"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/Contact"
            android:layout_width="203dp"
            android:layout_height="38dp"
            android:layout_margin="10dp"
            android:gravity="center"
            android:text="The Devlopers"
            android:textColor="@color/cardview_shadow_start_color"
            android:textSize="25sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="TextContrastCheck,TextContrastCheck" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/yohCard"
        android:layout_width="0dp"
        android:layout_height="160dp"
        android:layout_marginTop="25dp"
        android:layout_weight="1"
        android:background="@color/theme"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/userslist">

        <ImageView
            android:id="@+id/yohPic"
            android:layout_width="120dp"
            android:layout_height="151dp"
            android:layout_margin="10dp"
            android:background="#00FFFFFF"
            android:src="@drawable/male"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/yohWhatsapp"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:adjustViewBounds="false"
            android:background="#00FFFFFF"
            android:scaleType="centerInside"
            android:src="@drawable/whatsapp"
            app:layout_constraintBottom_toBottomOf="@+id/yohPic"
            app:layout_constraintStart_toEndOf="@+id/yohPic"
            app:layout_constraintTop_toBottomOf="@+id/yohMail"
            tools:ignore="SpeakableTextPresentCheck" />

        <TextView
            android:id="@+id/yohName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:layout_marginTop="16dp"
            android:text="Yohanan Kling"
            android:textColor="@color/white"
            android:textSize="25sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.602"
            app:layout_constraintStart_toEndOf="@+id/yohPic"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/yohMail"
            android:layout_width="158dp"
            android:layout_height="0dp"
            android:layout_below="@+id/yohName"
            android:layout_margin="10dp"
            android:layout_marginTop="15dp"
            android:autoLink="email"
            android:text="yohanankli@gmail.com"
            android:textColor="#212121"
            app:layout_constraintEnd_toEndOf="@+id/yohName"
            app:layout_constraintStart_toStartOf="@+id/yohName"
            app:layout_constraintTop_toBottomOf="@+id/yohName"
            tools:ignore="TouchTargetSizeCheck" />


        <ImageButton
            android:id="@+id/yohLinkedin"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="10dp"
            android:background="#00FFFFFF"
            android:scaleType="centerInside"
            android:src="@drawable/linkedin"
            app:layout_constraintBottom_toBottomOf="@+id/yohWhatsapp"
            app:layout_constraintStart_toEndOf="@+id/yohWhatsapp"
            app:layout_constraintTop_toTopOf="@+id/yohWhatsapp"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/yohGithub"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="10dp"
            android:background="#00FFFFFF"
            android:scaleType="centerInside"
            android:src="@drawable/github"
            app:layout_constraintBottom_toBottomOf="@+id/yohLinkedin"
            app:layout_constraintStart_toEndOf="@+id/yohLinkedin"
            app:layout_constraintTop_toTopOf="@+id/yohLinkedin"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/yohCv"
            android:layout_width="50dp"
            android:layout_height="48dp"
            android:layout_marginStart="10dp"
            android:background="#00FFFFFF"
            android:scaleType="centerInside"
            android:src="@drawable/cv"
            app:layout_constraintBottom_toBottomOf="@+id/yohGithub"
            app:layout_constraintStart_toEndOf="@+id/yohGithub"
            app:layout_constraintTop_toTopOf="@+id/yohGithub"
            tools:ignore="SpeakableTextPresentCheck" />

    </androidx.constraintlayout.widget.ConstraintLayout>

