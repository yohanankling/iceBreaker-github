            app:layout_constraintStart_toEndOf="@+id/tzachLinkedin"
            app:layout_constraintTop_toTopOf="@+id/tzachLinkedin"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/tzachCv"
            android:layout_width="50dp"
            android:layout_height="48dp"
            android:layout_marginStart="10dp"
            android:background="#00FFFFFF"
            android:scaleType="centerInside"
            android:src="@drawable/cv"
            app:layout_constraintBottom_toBottomOf="@+id/tzachGithub"
            app:layout_constraintStart_toEndOf="@+id/tzachGithub"
            app:layout_constraintTop_toTopOf="@+id/tzachGithub"
            tools:ignore="SpeakableTextPresentCheck" />

    </androidx.constraintlayout.widget.ConstraintLayout>

